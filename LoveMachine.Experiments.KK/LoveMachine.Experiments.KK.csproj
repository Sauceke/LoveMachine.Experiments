<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net35</TargetFramework>
    <AssemblyTitle>LoveMachine.Experiments</AssemblyTitle>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <AssemblyFileVersion>$(Version)</AssemblyFileVersion>
    <Company>Sauceke</Company>
    <Product>LoveMachine.Experiments</Product>
    <Copyright>2022</Copyright>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <OutputPath>..\bin\LoveMachine.Experiments.KK\BepInEx\plugins\KK_LoveMachine.Experiments\</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>
  <Target Name="EmbedVersion" BeforeTargets="BeforeBuild">
    <PropertyGroup>
      <!-- %3B is semicolon (;)  -->
      <_VersionTemplate>
// DO NOT EDIT this file - it is generated by the build
namespace LoveMachine.Experiments
{
    internal static class VersionInfo
    {
        internal const string Version = @"$(Version)"%3B
    }
}
      </_VersionTemplate>
    </PropertyGroup>
    <WriteLinesToFile File="VersionInfo.cs" Lines="$(_VersionTemplate)" WriteOnlyWhenDifferent="true" Overwrite="true" />
  </Target>
  <ItemGroup>
    <PackageReference Include="AllForOne" Version="1.0.0.27" PrivateAssets="all" />
    <PackageReference Include="IllusionModdingAPI.KKAPI" Version="1.34.0" PrivateAssets="all" ExcludeAssets="runtime" />
    <PackageReference Include="LitJson" Version="0.18.0" PrivateAssets="all" />
    <PackageReference Include="WebSocket4Net" Version="0.15.2" PrivateAssets="all" />
  </ItemGroup>
</Project>